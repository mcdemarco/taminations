<tamination>

  <tam title="Track Two"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" difficulty="1">
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="4" scaleX="5" />
    </path>
    <path>
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward 5" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="2" />
      <move select="Run Left" beats="4" offsetX="-1" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" />
      <move select="Run Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Left Track Two"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" difficulty="3">
    <path>
      <move select="Run Left" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward 5" beats="3" />
    </path>
    <path>
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Left" beats="4" scaleX="5" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" />
      <move select="Run Left" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Forward 2" beats="2" />
      <move select="Run Right" beats="4" offsetX="-1" />
    </path>
  </tam>

  <tam title="Track One"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" difficulty="3">
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward 3" beats="2" />
    </path>
    <path>
      <move select="Forward 2" beats="2" />
      <move select="Run Left" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" />
      <move select="Run Right" beats="4" scaleX="2" />
    </path>
  </tam>

  <tam title="Track Three"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" difficulty="3">
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="4" scaleX="5" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Extend Right" beats="4" scaleX="6" scaleY="2" />
    </path>
    <path>
      <move select="Forward 2" beats="2" />
      <move select="Run Left" beats="4" offsetX="-1" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" />
      <move select="Run Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
    </path>
  </tam>

  <tam title="Track Four"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" difficulty="3">
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" beats="4" scaleX="5" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward" />
      <move select="Extend Right" beats="4" scaleX="5" scaleY="2" />
    </path>
    <path>
      <move select="Forward 2" beats="2" />
      <move select="Run Left" beats="4" offsetX="-1" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" />
      <move select="Run Right" beats="4.5" scaleX="2" scaleY="2" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY="2" />
    </path>
  </tam>
</tamination>
