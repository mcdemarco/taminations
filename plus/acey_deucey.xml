    </path>
    <path>
      <move select="Flip Left" beats="4"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX=".6"/>
    </path>
  </tam>

  <tam title="Acey Deucey" from="3/4 Tag" formation="3/4 Tag" parts="1.5" difficulty="3">
    <path>
      <move select="Flip Left"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Swing Right"/>
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="Diamonds"
       formation="Diamonds RH Girl Points"
       parts="2" difficulty="3">
