    <path>
      <move select="Run Right" beats="4"/>
    </path>
    <path>
      <move select="Flip Left" beats="4"/>
    </path>
    <path>
      <move select="Run Right" beats="4"/>
    </path>
  </tam>

  <tam title="Acey Deucey" from="Trade By" formation="Trade By" difficulty="3">
