<tamination>

  <tam title="Acey Deucey"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="2" difficulty="1">
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Swing Left" beats="4" />
    </path>
    <path>
      <move select="Swing Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3" />
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="2" difficulty="1">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3" />
    </path>
    <path>
      <move select="Swing Right" beats="4" />
    </path>
    <path>
      <move select="Swing Right" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="2" difficulty="1">
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Swing Right" beats="4" />
    </path>
    <path>
      <move select="Swing Right" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3" />
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="2" difficulty="2">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3" />
    </path>
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="3" scaleY="3" />
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="Inverted Lines"
       formation="Inverted Lines Centers Out"
       parts="2" difficulty="2">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="Acey Deucey"
       from="3 and 1 Lines"
       formation="Formation boy -2 3 0 girl -2 1 0 boy -2 -1 0 girl -2 -3 180"
       parts="2" difficulty="2">
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="1.5" scaleY="3" />
    </path>
  </tam>

  <tam title="Acey Deucey" from="Completed Double Pass Thru"
