<tamination>

  <tam title="Follow Your Neighbor"
       from="Right-Hand Box"
       formation="Box RH"
       parts="" difficulty="1">
    <path>
      <move select="Forward 2" beats="1.5" offsetY="-.25" />
      <move select="Swing Right" scaleX=".75" scaleY=".75" />
      <move select="Hinge Right" offsetY=".25" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2" />
      <move select="Run Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="Follow Your Neighbor"
       from="Left-Hand Box"
       formation="Box LH"
       parts="" difficulty="2">
    <path>
      <move select="Lead Left" beats="3" scaleX="2" />
      <move select="Run Left" scaleY=".5" />
    </path>
    <path>
      <move select="Forward 2" beats="1.5" offsetY=".25" />
      <move select="Swing Left" scaleX=".75" scaleY=".75" />
      <move select="Hinge Left" offsetY="-.25" />
    </path>
  </tam>

  <tam title="Follow Your Neighbor"
       from="Right-Hand Waves"
