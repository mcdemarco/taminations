<tamination>

  <tam title="Fan the Top"
       from="Right-Hand Wave"
       formation="Wave RH"
       parts="" difficulty="1">
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Swing Left" beats="2.67" />
      <move select="Hinge Left" beats="1.33" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Left-Hand Wave"
       formation="Wave LH"
       parts="" difficulty="1">
    <path>
      <move select="Lead Left" beats="4" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Swing Right" beats="2.67" />
      <move select="Hinge Right" beats="1.33" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Right-Hand Two-Faced Line"
       formation="Two-Faced Line RH"
       parts="" difficulty="1">
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Swing Right" beats="2.67" />
      <move select="Hinge Right" beats="1.33" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Left-Hand Two-Faced Line"
       formation="Two-Faced Line LH"
       parts="" difficulty="1">
    <path>
      <move select="Swing Left" beats="2.67" />
      <move select="Hinge Left" beats="1.33" />
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX="3" scaleY="3" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Facing Couples"
       formation="Facing Couples"
       parts="" difficulty="2">
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
      <move select="Swing Left" />
      <move select="Hinge Left" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="" difficulty="1">
    <path>
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Left" beats="1.33" />
      <move select="Hinge Left" beats="1.33" />
      <move select="Hinge Left" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Hinge Left" beats="1.33" />
      <move select="Hinge Left" beats="1.33" />
      <move select="Hinge Left" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="3" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="" difficulty="1">
    <path>
      <move select="Lead Left" beats="4" scaleX="1.5" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX="1.5" scaleY="3" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="" difficulty="1">
    <path>
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" />
      <move select="Hinge Right" beats="1.33" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="3" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB"
       parts="" difficulty="1">
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Swing Left" beats="2.5" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Left" beats="2.5" scaleX=".5" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="1.5" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB"
       parts="" difficulty="1">
    <path>
      <move select="Lead Left" beats="4" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Swing Right" beats="2.5" scaleX=".5" scaleY=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Right" beats="2.5" scaleX=".5" scaleY=".5" />
      <move select="Hinge Right" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX="3" scaleY="1.5" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" difficulty="2">
    <path>
      <move select="Extend Left" />
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="2" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Swing Left" beats="2.5" />
      <move select="Hinge Left" scaleX=".5" />
    </path>
    <path>
      <move select="Extend Left" />
      <move select="Lead Right" beats="4" scaleX="1.5" scaleY="2" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Swing Left" beats="2.5" />
      <move select="Hinge Left" scaleX=".5" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Facing Lines"
       formation="Normal Lines"
       parts="" difficulty="2">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Hinge Left" scaleY=".5" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Right" beats="4" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Hinge Left" />
      <move select="Swing Left" />
    </path>
  </tam>

  <tam title="Fan the Top"
       from="Diamonds"
       formation="Diamonds RH Girl Points"
       parts="" difficulty="3">
    <path>
      <move select="Hinge Right" offsetY=".33" />
      <move select="Hinge Right" offsetX="-.33" />
      <move select="Hinge Right" offsetY="-.33" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Right" offsetY="-.33" />
      <move select="Hinge Right" offsetX=".33" />
      <move select="Hinge Right" offsetY=".33" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="3" />
    </path>
  </tam>
</tamination>
