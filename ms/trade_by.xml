<tamination>

  <tam title="Trade By"
       from="Trade By"
       formation="Trade By"
       parts="" difficulty="1">
    <path>
      <move select="Flip Left" />
    </path>
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="Trade By"
       from="3/4 Tag"
       formation="3/4 Tag"
       parts="" difficulty="2">
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleY="2" />
    </path>
    <path>
      <move select="Forward" beats="4" />
    </path>
  </tam>

  <tam title="Trade By"
       from="T-Bones"
       formation="T-Bone LUUR"
       parts="" difficulty="2">
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Flip Left" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" scaleY=".5" />
    </path>
  </tam>
</tamination>
