<tamination>

  <tam title="Double Pass Thru"
       from="Double Pass Thru"
       formation="Double Pass Thru"
       parts="" difficulty="1">
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Extend Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Extend Right" scaleY=".5" />
    </path>
  </tam>

  <tam title="Double Pass Thru"
       from="Quarter Tag"
       formation="Quarter Tag"
       parts="" difficulty="2">
    <path>
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Forward" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2.5" scaleX="2.5" scaleY=".5" />
      <move select="Extend Right" beats="1.5" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="1.5" scaleY=".5" />
    </path>
    <taminator>
      This is an example of the <a href="ocean_wave_rule.html">Ocean Wave</a> rule.
    </taminator>
  </tam>
</tamination>
