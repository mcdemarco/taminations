      <move select="Run Left" scaleX="2" scaleY="3" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="All 8 Circulate"
       from="3 and 1 Lines"
       formation="Formation girl -2 3 0 boy -2 1 180 girl -2 -1 180 boy -2 -3 180"
       parts="" difficulty="2">
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3" />
    </path>
  </tam>

  <tam title="All 8 Circulate"
       from="Inverted Lines"
       formation="Inverted Lines Centers Out"
       difficulty="2"
       parts="" >
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="All 8 Circulate" from="Trade By" formation="Trade By" difficulty="3">
    <path>
      <move select="Flip Left"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleY=".5"/>
      <move select="Extend Right" beats="1.5" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Left" beats="1.5" scaleY=".5"/>
      <move select="Extend Right" beats="1.5" scaleY=".5"/>
    </path>
    <taminator>
      Note that this is the same as Trade By.
    </taminator>
  </tam>

  <tam title="All 8 Circulate"
       from="T-Bones"
       formation="T-Bone DLDL"
       parts="" difficulty="3">
    <path>
      <move select="Lead Left 2" scaleY="1.5" beats="2" />
      <move select="Lead Left 2" scaleX="1.5" beats="2" />
    </path>
    <path>
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Run Right" beats="2" />
    </path>
    <path>
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Split Circulate"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="2" difficulty="1">
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
    </path>
  </tam>

  <tam title="Split Circulate"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="2" difficulty="1">
    <path>
      <move select="Run Left" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Run Left" beats="4" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="Split Circulate"
       from="Columns"
       formation="Column RH GBGB"
       parts="1.5" difficulty="2">
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Run Right" scaleX=".5" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
  </tam>

  <tam title="Split Circulate"
       from="T-Bones"
       formation="T-Bone DLDL"
       parts="" difficulty="3">
    <path>
      <move select="Run Left" />
    </path>
    <path>
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Run Left" />
    </path>
    <path>
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Split Circulate" from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru" difficulty="3">
    <path>
      <move select="Flip Left" offsetX="-1"/>
    </path>
    <path>
      <move select="Run Right" scaleY="2" offsetX="-1"/>
    </path>
    <path>
      <move select="Forward" beats="3"/>
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleY="2"/>
    </path>
  </tam>

  <tam title="Split Circulate 1 1/2"
       from="Waves"
       formation="Ocean Waves RH BGGB"
       parts="2;2" difficulty="2">
    <path>
      <move select="Forward 4" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Forward 4" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Run Right" beats="4" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Split Circulate Twice"
       from="Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="" difficulty="3">
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" scaleY=".75" />
    </path>
    <path>
      <move select="Flip Left" scaleY=".75" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
    </path>
  </tam>

  <tam title="Couples Circulate"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="2" difficulty="1">
    <path>
      <move select="Forward 4" hands="right" />
    </path>
    <path>
      <move select="Forward 4" hands="left" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="left" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="3" scaleY="3" hands="right" />
    </path>
  </tam>

  <tam title="Couples Circulate"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH"
       parts="2" difficulty="1">
    <path>
      <move select="Run Left" beats="4" scaleX="3" scaleY="3" hands="left" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="right" />
    </path>
    <path>
      <move select="Forward 4" hands="right" />
    </path>
    <path>
      <move select="Forward 4" hands="left" />
    </path>
  </tam>

  <tam title="Couples Circulate"
       from="T-Bone Couples"
       formation="T-Bone Couples"
       parts="" difficulty="2">
    <path>
      <move select="Extend Right" beats="2" hands="right" scaleX="2" />
      <move select="Extend Left" beats="2" hands="right" scaleX="2" />
    </path>
    <path>
      <move select="Forward 4" hands="left" />
    </path>
    <path>
      <move select="Run Left" beats="4" scaleX="3" scaleY="3" hands="left" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="right" />
    </path>
  </tam>

  <tam title="Column Circulate"
       from="Right-Hand Columns"
       formation="Column RH GBGB"
       parts="1.5" difficulty="1">
    <path>
      <move select="Run Right" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
  </tam>

  <tam title="Column Circulate"
       from="Left-Hand Columns"
       formation="Column LH GBGB"
       parts="1.5" difficulty="1">
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Run Left" />
    </path>
  </tam>
</tamination>
