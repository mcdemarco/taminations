<tamination>
  
  
  
  

  <tam title="Star Promenade"
       from="Static Square"
       formation="Static Square"
       parts="" >
    <path>
      <move select="Lead Right" hands="gripright" scaleX="2" />
      <move select="Swing Left" hands="both" beats="6" />
      <move select="Swing Left" hands="both" beats="6" />
    </path>
    <path>
      <move select="Quarter Right" hands="gripleft" offsetX=".75" offsetY="1" />
      <move select="Swing Left" beats="6" scaleX="2.25" scaleY="2.25" />
      <move select="Swing Left" beats="6" scaleX="2.25" scaleY="2.25" />
    </path>
    <path>
      <move select="Lead Right" hands="gripright" scaleX="2" />
      <move select="Swing Left" hands="both" beats="6" />
      <move select="Swing Left" hands="both" beats="6" />
    </path>
    <path>
      <move select="Quarter Right" hands="gripleft" offsetX=".75" offsetY="1" />
      <move select="Swing Left" beats="6" scaleX="2.25" scaleY="2.25" />
      <move select="Swing Left" beats="6" scaleX="2.25" scaleY="2.25" />
    </path>
  </tam>
