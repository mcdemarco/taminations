<tamination>

  <tam title="Swing Thru"
       from="Right-Hand Wave"
       formation="Wave RH"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Stand" beats="3" hands="none"/>
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Left-Hand Wave"
       formation="Wave LH"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" hands="none"/>
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
  </tam>

  <tam title="Swing Thru" from="Facing Couples"
       formation="Facing Couples"
       difficulty="2">
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
      <move select="Swing Right"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Forward 2" beats="3"/>
      <move select="Swing Right"/>
      <move select="Stand" beats="3"/>
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Swing Left" />
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Stand" beats="3" hands="none"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Stand" beats="3" hands="left"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
    <path>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
    </path>
  </tam>

  <tam title="Swing Thru"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" difficulty="1">
    <path>
      <move select="Extend Left" scaleY="2" />
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Extend Left" scaleY="2" />
      <move select="Swing Right" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Swing Right" />
    </path>
    <taminator>
      This is an example of the <a href="facing_couples_rule.html">Facing Couples</a> rule.
    </taminator>
  </tam>

  <tam title="Swing Thru" from="Quarter Tag" formation="Quarter Tag" parts="1.5;1.5;1.5" difficulty="1">
    <path>
    </path>
    <path>
    </path>
    <path>
      <move select="Swing Right"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Swing Right"/>
      <move select="Stand" beats="3" hands="none"/>
    </path>
    <taminator>
      The center dancers do not extend to the outside dancers.
      The <a href="facing_couples_rule.html">Facing Couples</a> rule does not apply
      here because the center dancers are already in a wave.
    </taminator>
  </tam>

  <tam title="Alamo Swing Thru"
       from="Alamo Wave"
       formation="Alamo Wave"
       parts="1.6;1.6;1.6" difficulty="1">
    <path>
      <move select="Sxtnth Left" />
      <move select="Swing Right" />
      <move select="Sxtnth Left" />
      <move select="Sxtnth Left" />
      <move select="Swing Left" />
      <move select="Sxtnth Left" />
    </path>
    <path>
      <move select="Sxtnth Right" />
      <move select="Swing Right" />
      <move select="Sxtnth Right" />
      <move select="Sxtnth Right" />
      <move select="Swing Left" />
      <move select="Sxtnth Right" />
    </path>
    <path>
      <move select="Sxtnth Left" />
      <move select="Swing Right" />
      <move select="Sxtnth Left" />
      <move select="Sxtnth Left" />
      <move select="Swing Left" />
      <move select="Sxtnth Left" />
    </path>
    <path>
      <move select="Sxtnth Right" />
      <move select="Swing Right" />
      <move select="Sxtnth Right" />
      <move select="Sxtnth Right" />
      <move select="Swing Left" />
      <move select="Sxtnth Right" />
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Left-Hand Wave"
       formation="Wave LH"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Stand" beats="3" hands="none"/>
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Right-Hand Wave"
       formation="Wave RH"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" hands="none"/>
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Stand" beats="3" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Stand" beats="3" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Swing Right" />
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Left-Hand Tidal Wave"
       formation="Tidal Wave LH BGGB"
       parts="1.5;1.5;1.5" difficulty="1">
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Stand" beats="3" hands="nohands" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Stand" beats="3" hands="nohands" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Right-Hand Tidal Wave"
       formation="Tidal Wave RH BGGB"
       parts="1.5;1.5;1.5" difficulty="2">
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
    <path>
      <move select="Swing Left" scaleX=".75" scaleY=".5" hands="gripleft"/>
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Swing Right" scaleX=".75" scaleY=".5" hands="gripright"/>
    </path>
  </tam>

  <tam title="Left Swing Thru"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" difficulty="2">
    <path>
      <move select="Forward" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Extend Right" scaleY="2" />
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Forward" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Extend Right" scaleY="2" />
      <move select="Swing Left" />
      <move select="Swing Right" />
    </path>
    <taminator>
      This is an example of the <a href="facing_couples_rule.html">Facing Couples</a> rule.
      Because this is a <b>Left</b> Swing Thru, dancers first extend to
      a Left-Hand Wave.
    </taminator>
  </tam>

  <tam title="Alamo Left Swing Thru"
       from="Alamo Wave"
       formation="Alamo Wave"
       parts="1.6;1.6;1.6" difficulty="2">
    <path>
      <move select="Sxtnth Right" />
      <move select="Swing Left" />
      <move select="Sxtnth Right" />
      <move select="Sxtnth Right" />
      <move select="Swing Right" />
      <move select="Sxtnth Right" />
    </path>
    <path>
      <move select="Sxtnth Left" />
      <move select="Swing Left" />
      <move select="Sxtnth Left" />
      <move select="Sxtnth Left" />
      <move select="Swing Right" />
      <move select="Sxtnth Left" />
    </path>
    <path>
      <move select="Sxtnth Right" />
      <move select="Swing Left" />
      <move select="Sxtnth Right" />
      <move select="Sxtnth Right" />
      <move select="Swing Right" />
      <move select="Sxtnth Right" />
    </path>
    <path>
      <move select="Sxtnth Left" />
      <move select="Swing Left" />
      <move select="Sxtnth Left" />
      <move select="Sxtnth Left" />
      <move select="Swing Right" />
      <move select="Sxtnth Left" />
    </path>
  </tam>

</tamination>
