<tamination>
  
  

  <tam title="Left Shakedown"
       from="Trade By"
       formation="Trade By"
       parts="" >
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Left Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="" >
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY=".5" />
      <move select="Lead Left" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY="1" />
      <move select="Lead Left" scaleX="1.5" />
    </path>
  </tam>

  <tam title="As Couples Left Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="" >
    <path>
      <move select="Run Left" hands="left" scaleX="3" scaleY="3" offsetX="1" />
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetY="-1" />
    </path>
    <path>
      <move select="Run Left" hands="right" offsetX="1" />
      <move select="Quarter Left" hands="right" offsetY="-1" />
    </path>
    <path>
      <move select="Belle Wheel" offsetX="-2" offsetY="1" />
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="Beau Wheel" offsetX="-2" offsetY="1" />
      <move select="Quarter Left" hands="right" offsetX="1" />
    </path>
  </tam>

  <tam title="Concentric Left Shakedown"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" >
    <path>
      <move select="Flip Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="2" />
      <move select="Quarter Left" offsetX="2" />
    </path>
    <path>
      <move select="Flip Left" scaleY=".67" />
      <move select="Quarter Left" offsetY="-.67" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Stretch Left Shakedown"
       from="Trade By"
       formation="Trade By"
       parts="" >
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="1" />
      <move select="Quarter Left" offsetX="2" />
    </path>
    <path>
      <move select="Flip Left" offsetX="1" />
      <move select="Quarter Left" offsetX="-1" offsetY="-1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="1" />
      <move select="Quarter Left" offsetX="1" />
    </path>
  </tam>

  <tam title="Stretch Left Shakedown"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="" >
    <path>
      <move select="Flip Left" scaleY="2" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY="2.5" />
      <move select="Lead Left" scaleX="1.5" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" />
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1.5" offsetY="1" />
      <move select="Lead Left" scaleX="1.5" />
    </path>
  </tam>
</tamination>
