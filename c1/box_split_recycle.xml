<tamination>

  <tam title="Box Recycle"
       from="Right-Hand Box"
       formation="Box RH"
       parts="" >
    <path numbers="1 4">
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" />
    </path>
    <path numbers="2 3">
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2" offsetX="-2" />
    </path>
  </tam>

  <tam title="Box Recycle"
       from="Left-Hand Box"
       formation="Box LH"
       parts="" >
    <path numbers="1 4">
      <move select="Run Left" beats="4" hands="gripleft" scaleX="2" scaleY="2" offsetX="-2" />
    </path>
    <path numbers="2 3">
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="2" />
    </path>
  </tam>

  <tam title="Split Recycle"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGBG"
       parts="" >
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
  </tam>

  <tam title="Split Recycle"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGBG"
       parts="" >
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="-1.5" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="-1.5" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="2" offsetY=".5" />
    </path>
  </tam>

  <tam title="Split Recycle"
       from="Right-Hand Columns"
       formation="Column RH GBGB"
       parts="" >
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="1" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleY="2" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="1" />
    </path>
  </tam>

  <tam title="Split Recycle"
       from="Left-Hand Columns"
       formation="Column LH GBGB"
       parts="" >
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleY="2" offsetX="-1" />
    </path>
    <path>
      <move select="U-Turn Left" beats="4" hands="gripleft" offsetX="1" />
    </path>
    <path>
      <move select="Run Left" beats="4" hands="gripleft" scaleX="2" scaleY="2" offsetX="-1" />
    </path>
  </tam>

  <tam title="Split Recycle"
       from="3 and 1 Lines"
       formation="Formation boy -2 3 0 girl -2 1 180 boy -2 -1 0 girl -2 -3 0"
       parts="" >
    <path numbers="5 4">
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
    <path numbers="6 3">
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
    <path numbers="1 8">
      <move select="U-Turn Right" beats="4" offsetX="2" offsetY="-.5" />
    </path>
    <path numbers="2 7">
      <Movement hands="no" beats="4" cx1="-1" cy1="2" cx2="1" cy2="2.5" x2="2" y2="2.5" cx3="1" cx4="2" cy4="2" x4="3" y4="2" />
    </path>
  </tam>

  <tam title="As Couples Box Recycle" from="Two-Faced Lines"
       formation="Two-Faced Lines RH" group=" ">
    <path>
      <move select="Run Right" hands="gripright" beats="4" offsetX="2" scaleY=".5" offsetY="-1.5"/>
    </path>
    <path>
      <move select="BackRun Left" hands="gripboth" beats="4" offsetX="2" scaleY=".5" offsetY="-.5"/>
    </path>
    <path>
      <move select="Run Right" hands="gripboth" beats="4" scaleX="2" offsetX="-2" scaleY=".5" offsetY="-2.5"/>
    </path>
    <path>
      <move select="Run Right" hands="gripright" beats="4" scaleX="3" offsetX="-2" scaleY=".5" offsetY="-5.5"/>
    </path>
  </tam>

  <tam title="All 8 Box Recycle" from="" formation="Static MiniWaves RH" group=" ">
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="3" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2" offsetX="-3" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="3" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2" offsetX="-3" />
    </path>
  </tam>

  <tam title="Concentric Box Recycle" from="" formation="Column RH GBGB" group=" ">
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2.25" offsetX="-3" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleY="2" offsetX="-1" offsetY="1.5"/>
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="1" offsetY="-.5"/>
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="3" offsetY="1.5"/>
    </path>
  </tam>

  <tam title="Tandem Box Recycle" from="" formation="Column RH GBGB" group=" ">
    <path>
      <move select="Quarter Right" hands="gripright" beats="2" offsetY="-4"/>
      <move select="Quarter Right" hands="gripright" beats="2" offsetY="-5"/>
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="2" scaleY="2" offsetX="1" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="-1" />
    </path>
    <path>
      <move select="Quarter Right" hands="gripright" beats="2" offsetX="2" offsetY="2"/>
      <move select="Quarter Right" hands="gripright" beats="2" offsetX="2" offsetY="3"/>
    </path>
  </tam>

  <tam title="Triple Box Recycle" from="" formation="Phantom Waves RH" group=" ">
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
    <path>
      <move select="Run Right" beats="4" hands="gripright" scaleX="1.5" scaleY="2" offsetX="-2" offsetY="1.5" />
    </path>
    <path>
      <move select="U-Turn Right" beats="4" hands="gripright" offsetX="2" offsetY="-.5" />
    </path>
  </tam>

</tamination>
