<tamination>

  <comment>
    <p>Stretch versions of Challenge calls are listed with each call.</p>
  </comment>

  <tam title="Stretch Bend the Line" group="Stretch"
       from=""
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Extend Left" beats="1.5" scaleX="2" scaleY=".5" />
      <move select="Lead Right" scaleY="1.5" />
    </path>
    <path>
      <move select="BackHinge Left" />
    </path>
    <path>
      <move select="BackHinge Right" />
    </path>
    <path>
      <move select="Lead Left" beats="3" scaleX="3" />
    </path>
  </tam>

  <tam title="Stretch Cast Off 3/4" group="Stretch"
       from=""
       formation="Lines Facing Out"
       parts="" >
    <path>
      <move select="Quarter Right" hands="left" offsetX=".33" offsetY=".33" />
      <move select="Quarter Right" hands="left" offsetX="-.33" offsetY=".33" />
      <move select="Quarter Right" offsetX="-.33" offsetY="-.33" />
    </path>
    <path>
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX=".33" offsetY=".33" />
      <move select="Hinge Right" scaleX="2" scaleY="2" offsetX="-.33" offsetY=".33" />
      <move select="Forward" beats="1" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Right" scaleX="2" scaleY="2" offsetX=".17" offsetY="-.33" />
    </path>
    <path>
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetX=".33" offsetY="-.33" />
      <move select="Hinge Left" scaleX="2" scaleY="2" offsetX="-.33" offsetY="-.33" />
      <move select="Lead Left" beats="2" scaleX="2" scaleY="2" offsetX="1.67" offsetY=".33" />
    </path>
    <path>
      <move select="Quarter Left" hands="right" offsetX=".33" offsetY="-.33" />
      <move select="Quarter Left" hands="right" offsetX="-.33" offsetY="-.33" />
      <move select="Quarter Left" offsetX="-.33" offsetY=".33" />
    </path>
  </tam>

  <tam title="Stretch Chase Right" group="Stretch"
       from=""
       formation="Trade By"
       parts="" >
    <path>
      <move select="U-Turn Right" beats="1.5" />
      <move select="Forward 4" />
      <move select="Flip Right" beats="1.5" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="U-Turn Right" beats="1.5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="1.5" />
    </path>
    <path>
      <move select="Flip Right" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Stretch Crossfire" group="Stretch"
       from=""
       formation="Two-Faced Lines RH"
       parts="" >
    <path>
      <move select="Lead Right" beats="4" scaleX="3" scaleY="2" />
      <move select="Forward" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path>
      <move select="Hinge Right" scaleX=".5" />
      <move select="Hinge Right" scaleY=".5" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Swing Right" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Cross Fold Right" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Stretch Cross Trail Thru" group="Stretch"
       from=""
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Stand" />
      <move select="Extend Right" beats="5" scaleX="2" scaleY="4.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Stand" />
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Left" beats="4" scaleX="2" scaleY="3.5" />
    </path>
  </tam>

  <tam title="Stretch Flutterwheel (from Eight Chain Thru)" group="Stretch"
       from=""
       formation="Eight Chain Thru"
       parts="" >
    <path>
      <move select="Stand" hands="nohands" beats="3.5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Run Right" hands="right" offsetX="1" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="1.5" />
      <move select="Swing Right" hands="both" scaleX=".5" scaleY=".5" />
      <move select="U-Turn Right" hands="left" offsetX="1" offsetY=".5" />
    </path>
    <path>
      <move select="Stand" hands="nohands" beats="3.5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5" />
      <move select="Run Right" hands="right" offsetX="1" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="1.5" />
      <move select="Swing Right" hands="both" scaleX=".5" scaleY=".5" />
      <move select="Forward 2" />
      <move select="U-Turn Right" hands="left" offsetX="1" offsetY=".5" />
    </path>
  </tam>

  <tam title="Stretch Flutterwheel (from Lines)" group="Stretch"
       from=""
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Forward" hands="nohands" beats="3.5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleX="1.5" scaleY="1.5" />
      <move select="Run Right" hands="right" scaleX="1.5" scaleY="2" offsetX="1.5" offsetY="-1.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY="1.75" />
      <move select="Swing Right" hands="both" scaleX=".75" scaleY=".75" />
      <move select="U-Turn Right" hands="left" offsetX="1.5" offsetY="2.25" />
    </path>
    <path>
      <move select="Forward" hands="nohands" beats="3.5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleX="1.5" scaleY="1.5" />
      <move select="Run Right" hands="right" scaleX="1.5" scaleY="2" offsetX="1.5" offsetY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="1.5" scaleY="1.75" />
      <move select="Swing Right" hands="both" scaleX=".75" scaleY=".75" />
      <move select="U-Turn Right" hands="left" offsetX="1.5" offsetY=".25" />
    </path>
  </tam>

  <tam title="Stretch Hinge" group="Stretch"
       from=""
       formation="Ocean Waves RH BGBG"
       parts="" >
    <path>
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Stretch Linear Cycle" group="Stretch"
       from=""
       formation="Ocean Waves RH BGGB"
       parts="" >
    <path>
      <move select="Hinge Right" />
      <move select="Forward 4" />
      <move select="Run Right" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Run Right" />
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2" />
      <move select="U-Turn Right" offsetX="1" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Run Right" />
      <move select="Forward 2" />
      <move select="U-Turn Right" offsetX="2" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Forward 4" />
      <move select="Run Right" beats="4" scaleY="2" />
    </path>
  </tam>

  <tam title="Stretch Partner Hinge" group="Stretch"
       from=""
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1" />
    </path>
  </tam>

  <tam title="Stretch Partner Trade" group="Stretch"
