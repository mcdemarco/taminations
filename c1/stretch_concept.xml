    </path>
    <path>
      <move select="Run Right" beats="4" scaleY="2" />
    </path>
  </tam>

  <tam title="Stretch Recycle (from Right-Hand Waves)" group="Stretch"
       formation="Ocean Waves RH BGGB">
    <path numbers="5 4">
      <move select="Run Right" beats="4" offsetX="3" offsetY="-2" />
    </path>
    <path numbers="6 3">
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".5" offsetX="-1.5" />
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".5" offsetX="1.5" />
    </path>
    <path numbers="7 2">
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".5" offsetX="-.5" />
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".5" offsetX=".5" />
    </path>
    <path numbers="8 1">
      <move select="Run Right" beats="4" offsetX="1" offsetY="-2" />
    </path>
  </tam>

  <tam title="Stretch Recycle (from Left-Hand Waves)" group="Stretch"
       from=""
       formation="Ocean Waves LH BGGB"
       parts="" >
    <path>
      <move select="Run Left" beats="4" offsetX="1" offsetY="2" />
    </path>
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" offsetX="-.5" />
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" offsetX=".5" />
    </path>
    <path>
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" offsetX="-1.5" />
      <move select="Run Left" beats="2" scaleX=".5" scaleY=".5" offsetX="1.5" />
