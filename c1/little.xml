<tamination>

  <tam title="Little"
       from="Right-Hand Quarter Tag"
       formation="Quarter Tag"
       parts="" >
    <path>
      <move select="Extend Right" beats="6" scaleY="4" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Flip Right" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Little"
       from="Left-Hand Quarter Tag"
       formation="Quarter Tag LH"
       parts="" >
    <path>
      <move select="Extend Right" beats="6" scaleY="4" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Flip Left" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Little"
       from="Right-Hand 3/4 Tag"
       formation="3/4 Tag"
       parts="" >
    <path numbers="1 8">
      <move select="Flip Right" offsetX="-1" />
      <move select="Forward 4" />
    </path>
    <path numbers="2 7">
      <move select="Flip Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward" beats="2" />
    </path>
    <path numbers="6 3">
      <move select="Flip Right" beats="7" offsetX="2" />
    </path>
    <path numbers="5 4">
      <move select="Forward 2" beats="7" />
    </path>
  </tam>

  <tam title="Little"
       from="Left-Hand 3/4 Tag"
       formation="3/4 Tag LH"
       parts="" >
    <path>
      <move select="Flip Right" offsetX="-1" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Flip Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward" beats="2" />
    </path>
    <path>
      <move select="Flip Left" beats="7" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="7" />
    </path>
  </tam>

  <tam title="Little"
       from="Right-Hand Quarter Waves"
       formation="Quarter Wave RH"
       parts="" >
    <path>
      <move select="Lead Right" scaleX=".5" />
      <move select="Lead Left" beats="3.5" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Right" scaleX=".5" />
      <move select="Lead Right" beats="3.5" scaleX="3" scaleY="1.5" />
    </path>
    <path>
      <move select="Flip Right" beats="5" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="5" />
    </path>
  </tam>

  <tam title="Little"
       from="Left-Hand Quarter Waves"
       formation="Quarter Wave LH"
       parts="" >
    <path>
      <move select="Flip Right" beats="2" offsetX="-1" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="1.5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Little"
       from="Diamonds"
       formation="Diamonds RH Girl Points" >
    <path numbers="6 3">
      <move select="Flip Right" beats="6" offsetX="1" />
    </path>
    <path numbers="2 7">
      <move select="Quarter Right"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5"/>
      <move select="Quarter Left" offsetX="1" offsetY="-.5"/>
    </path>
    <path numbers="5 4">
      <move select="Forward" beats="6" />
    </path>
    <path numbers="8 1">
      <move select="Quarter Right"/>
      <move select="Extend Left" beats="3" scaleX="3" scaleY="1.5"/>
      <move select="Lead Right" scaleY=".5"/>
    </path>
  </tam>

  <tam title="Little, Points Go As You Are"
       from="Diamonds"
       formation="Diamonds RH Girl Points" >
    <path numbers="6 3">
      <move select="Flip Right" beats="6" offsetX="2" />
    </path>
    <path numbers="2 7">
      <move select="Forward 4" />
      <move select="Lead Right" beats="2" />
    </path>
    <path numbers="5 4">
      <move select="Forward 2" beats="6" />
    </path>
    <path numbers="8 1">
      <move select="Lead Right" beats="2" />
      <move select="Forward 4" />
    </path>
  </tam>

  <tam title="Little, Outsides Out"
       from="Right-Hand 3/4 Tag"
       formation="3/4 Tag"
       parts="" >
    <path numbers="1 8">
      <move select="Run Right" offsetX="-1" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
    </path>
    <path numbers="2 7">
      <move select="Run Left" offsetX="-1"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
    </path>
    <path numbers="6 3">
      <move select="Flip Right" beats="7" offsetX="2" />
    </path>
    <path numbers="5 4">
      <move select="Forward 2" beats="7" />
    </path>
  </tam>


  <tam title="Left Little"
       from="Right-Hand Quarter Tag"
       formation="Quarter Tag"
       parts="" >
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleY="4" />
    </path>
    <path>
      <move select="Flip Right" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Left Little"
       from="Left-Hand Quarter Tag"
       formation="Quarter Tag LH"
       parts="" >
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleY="4" />
    </path>
    <path>
      <move select="Flip Left" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Left Little"
       from="Right-Hand 3/4 Tag"
       formation="3/4 Tag"
       parts="" >
    <path>
      <move select="Flip Left" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward" beats="2" />
    </path>
    <path>
      <move select="Flip Left" offsetX="-1" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Flip Right" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Left Little"
       from="Left-Hand 3/4 Tag"
       formation="3/4 Tag LH"
       parts="" >
    <path>
      <move select="Flip Left" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward" beats="2" />
    </path>
    <path>
      <move select="Flip Left" offsetX="-1" />
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Flip Left" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

  <tam title="Left Little"
       from="Right-Hand Quarter Waves"
       formation="Quarter Wave RH"
       parts="" >
    <path>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="2.5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Left" beats="2" offsetX="-1" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Flip Right" beats="6" offsetX="2" />
    </path>
    <path>
      <move select="Forward 2" beats="6" />
    </path>
  </tam>

</tamination>
