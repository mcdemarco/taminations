<tamination>



  <tam title="Left Wheel Fan Thru"
       from="Facing Couples"
       formation="Facing Couples"
       parts="" >
    <path>
      <move select="Quarter Left" offsetX="1" offsetY="1" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.75" offsetY=".25" />
      <move select="Swing Left" scaleX=".707" scaleY=".707" />
      <move select="HalfHinge Left" beats="2" offsetX="1.4" offsetY="1.2" />
    </path>
  </tam>

  <tam title="Left Wheel Fan Thru"
       from="Lines"
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
    <path>
      <move select="Quarter Left" offsetX="1" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
  </tam>

  <tam title="Left Wheel Fan Thru"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" >
    <path>
      <move select="Quarter Left" offsetY="1" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX=".6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.4" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Left" offsetY="1" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX=".6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.4" offsetY="1" />
    </path>
  </tam>

  <tam title="All 8 Left Wheel Fan Thru" from="Static Square" group=" " formation="Static Square">
    <path>
      <move select="Lead Left" beats="6" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 3"/>
      <move select="Cast Left"/>
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Lead Left" beats="6" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 3"/>
      <move select="Cast Left"/>
      <move select="Forward 3"/>
    </path>
  </tam>

  <tam title="As Couples Left Wheel Fan Thru"
       from="" group=" "
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="BackHinge Right" />
    </path>
    <path>
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Cast Left" scaleX=".5" scaleY=".5" />
      <move select="Extend Right" hands="right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" hands="left" beats="2" scaleX="2" scaleY="1.5" />
      <move select="Cast Left" scaleX="1.5" scaleY="1.5" />
      <move select="Extend Right" hands="left" beats="2" scaleX="2" scaleY="1.5" />
    </path>
  </tam>

  <tam title="Butterfly Left Wheel Fan Thru" from="" formation="Butterfly Chain Thru" group=" ">
    <path>
      <move select="Quarter Left"/>
      <move select="Stand" hands="none" beats="5.5"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX=".1" offsetY="1.4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.05" offsetY="1.35" />
    </path>
    <path>
      <move select="Quarter Left"/>
      <move select="Stand" hands="none" beats="5.5"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.1" offsetY="1.4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.75" offsetY=".65" />
    </path>
  </tam>

  <tam title="&quot;O&quot; Left Wheel Fan Thru" from="" formation="O Eight Chain Thru" group=" ">
    <path>
      <move select="Quarter Left"/>
      <move select="Stand" hands="none" beats="5.5"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.1" offsetY="1.4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.75" offsetY=".65" />
    </path>
    <path>
      <move select="Quarter Left"/>
      <move select="Stand" hands="none" beats="5.5"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX=".1" offsetY="1.4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="1.05" offsetY="1.35" />
    </path>
  </tam>

  <tam title="Stretch Left Wheel Fan Thru"
       from=""
       formation="Normal Lines"
       group=" " >
    <path>
      <move select="Quarter Left" offsetX="1" beats="3"/>
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX="0" offsetY="1" />
      <move select="Extend Right" beats="3" scaleX="2"/>
    </path>
    <path>
      <move select="Lead Left" scaleY="2" beats="3"/>
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
  </tam>

  <tam title="Tandem Left Wheel Fan Thru" from="" formation="Double Pass Thru" group=" ">
    <path>
      <move select="Stand" beats="8" hands="none"/>
      <move select="Quarter Left" beats="3" offsetX="2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetX="2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetX="2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetX="2"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetY="2"/>
      <move select="Stand" beats="8" hands="none"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetY="2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetY="2"/>
      <move select="Quarter Left" beats="2" hands="gripleft" offsetY="2"/>
      <move select="Forward 2"/>
    </path>
  </tam>

  <tam title="Triple Box Left Wheel Fan Thru" from="" formation="Phantom Lines In" group=" ">
    <path>
      <move select="Quarter Left" offsetX="1" beats="3"/>
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
    <path>
      <move select="Quarter Left" offsetX="1" beats="3"/>
      <move select="Stand" hands="none" beats="4"/>
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
    <path>
      <move select="HalfHinge Left" beats="2" offsetX="1.6" offsetY=".4" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
      <move select="HalfHinge Left" beats="2" offsetX=".7" offsetY=".3" />
    </path>
    <path>
      <move select="Quarter Left" offsetX="1" beats="3"/>
      <move select="Stand" hands="none" beats="4"/>
    </path>
  </tam>

</tamination>
