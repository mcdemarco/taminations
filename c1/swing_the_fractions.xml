<tamination>

  <tam title="Swing the Fractions"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="1.5;3;4.5;3" >
    <path numbers="1 8">
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
    <path numbers="2 7">
      <move select="Hinge Right" />
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
    <path numbers="3 6">
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Stand" beats="3" />
      <move select="Hinge Right" />
    </path>
    <path numbers="4 5">
      <move select="Hinge Right" />
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
      <move select="Stand" beats="3" />
      <move select="Hinge Right" />
    </path>
  </tam>

  <tam title="Swing the Fractions"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="1.5;3;4.5;3" >
    <path numbers="1 8">
      <move select="Stand" beats="9" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
    <path numbers="2 7">
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Stand" beats="1.5" />
    </path>
    <path numbers="3 6">
      <move select="Hinge Right" />
      <move select="Stand" beats="3" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Stand" beats="1.5" />
    </path>
    <path numbers="4 5">
      <move select="Stand" beats="9" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
  </tam>

  <tam title="Swing the Fractions"
       from="Facing Diamonds"
       formation="Diamonds Facing Girl Points"
       parts="1.5;3;4.5;3" >
    <path>
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Stand" beats="9" />
    </path>
    <path>
      <move select="Stand" beats="1.5" />
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Stand" beats="3" />
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Stand" beats="9" />
    </path>
    <path>
      <move select="Stand" beats="1.5" />
      <move select="Swing Left" />
      <move select="Cast Right" />
      <move select="Swing Left" />
      <move select="Hinge Right" />
    </path>
  </tam>

  <tam title="Left Swing the Fractions"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB"
       parts="1.5;3;4.5;3" >
    <path>
      <move select="Stand" beats="9" />
      <move select="Swing Right" />
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Stand" beats="1.5" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Stand" beats="1.5" />
    </path>
    <path>
      <move select="Stand" beats="9" />
      <move select="Swing Right" />
      <move select="Hinge Left" />
    </path>
  </tam>

  <tam title="Left Swing the Fractions"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB"
       parts="1.5;3;4.5;3" >
    <path>
      <move select="Hinge Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
      <move select="Stand" beats="3" />
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Stand" beats="3" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Swing Right" />
      <move select="Cast Left" />
      <move select="Swing Right" />
      <move select="Hinge Left" />
    </path>
  </tam>
</tamination>
