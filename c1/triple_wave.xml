<tamination>
  
  

  <tam title="Triple Line Bend the Line" group="Triple Line"
       from=""
       formation="Formation boy -4 1 0 girl -4 -1 0 boy 0 1 0 girl 0 3 0 phantom -4 3 0 phantom -4 -3 0">
    <path>
      <move select="BackHinge Left"/>
    </path>
    <path>
      <move select="BackHinge Right"/>
    </path>
    <path>
      <move select="BackHinge Left"/>
    </path>
    <path>
      <move select="Hinge Right"/>
    </path>
    <path>
      <move select="Hinge Right"/>
    </path>
    <path>
      <move select="Hinge Left"/>
    </path>
  </tam>

  <tam title="Triple Wave Swing Thru" group="Triple Wave" from=""
       formation="Formation boy -4 1 180 girl -4 -1 0 boy 0 1 180 girl 0 3 0 phantom -4 3 0 phantom -4 -3 180">
    <path>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Swing Right"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Swing Right"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Swing Right"/>
      <move select="Swing Left"/>
    </path>
  </tam>

</tamination>
