<tamination>

  <tam title="O Circulate"
       from="Right-Hand O"
       formation="O RH"
       parts="1.5" >
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="O Circulate"
       from="Left-Hand O"
       formation="O LH"
       parts="1.5" >
    <path>
      <move select="Flip Left" />
    </path>
    <path>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Forward 2" beats="3" />
    </path>
  </tam>
</tamination>
