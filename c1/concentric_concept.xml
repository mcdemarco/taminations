<tamination>

  <comment>
    <p>Concentric versions of Challenge calls are listed with each call.</p>
  </comment>

  <tam title="From columns, Concentric Box Counter Rotate" group="From columns, Concentric"
       formation="Magic Column RH">
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="4" offsetY="-2"/>
    </path>
    <path>
      <move select="Quarter Right" hands="gripright" beats="3" offsetY="-2"/>
    </path>
    <path>
      <move select="Quarter Right" hands="gripright" beats="3" offsetX="2"/>
    </path>
    <path>
      <move select="Quarter Left" hands="gripleft" beats="5" offsetX="-2" offsetY="4"/>
    </path>
  </tam>

  <tam title="From columns, Concentric Box Transfer" group="From columns, Concentric"
       formation="Column RH GBGB">
    <path>
      <move select="Run Right" beats="6" scaleY="2.5"/>
      <move select="Forward 3" beats="5"/>
      <move select="Lead Right"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX=".5" scaleY="1.25" />
      <move select="Forward" />
      <move select="Lead Right" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Right" beats="1.5" scaleY=".25" />
      <move select="Cast Right" scaleX=".75" scaleY=".75" />
      <move select="Extend Left" beats="2" scaleY=".25" />
    </path>
    <path>
      <move select="Extend Left" beats="3" scaleX="3" scaleY="2"/>
      <move select="Run Right" beats="6" scaleX="3" scaleY="3"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="3"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY="2"/>
    </path>
  </tam>

  <tam title="From columns, Concentric Chase Right" group="From columns, Concentric"
       formation="Completed Double Pass Thru">
    <path>
      <move select="Run Right" offsetX="-1"/>
      <move select="Forward 4"/>
      <move select="Run Right" beats="5" scaleY="2" offsetX="1"/>
    </path>
    <path>
      <move select="Run Right" beats="5" scaleY="2" offsetX="-1"/>
      <move select="Forward 4"/>
      <move select="Extend Right" beats="3" scaleY="2"/>
    </path>
    <path>
      <move select="U-Turn Right" beats="1.5" offsetX="-1"/>
      <move select="Forward" />
      <move select="Flip Right" beats="2.5" />
    </path>
    <path>
      <move select="Run Right" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="From columns, Concentric Dixie Style to a Wave"
       group="From columns, Concentric"
       formation="Double Pass Thru">
    <path>
      <move select="Quarter Right" offsetX="-.5" offsetY="-1" beats="10"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="2" scaleY="2"/>
      <move select="Lead Right" beats="4" scaleX="2" scaleY="2"/>
      <move select="Lead Right" beats="4" scaleX="2.5" scaleY="3"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Quarter Right" beats="3" offsetX="-.5" offsetY="-1" />
      <move select="Swing Left" scaleX=".5" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="1.5" hands="right" offsetY=".5" />
      <move select="Hinge Right" scaleX=".5" scaleY=".5" />
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
    </path>
  </tam>

  <tam title="From columns, Concentric Do Paso" group="From columns, Concentric"
       formation="Double Pass Thru">
    <path>
      <move select="Quarter Right" offsetX="-.5" offsetY="-1"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4" scaleX="3.5" scaleY="2.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Left" beats="3" scaleX="3.5" scaleY="1.5"/>
      <move select="U-Turn Left" offsetX="1"/>
      <move select="Beau Wheel" scaleX=".5" scaleY=".5"/>
      <move select="BackHinge Right" scaleY=".5"/>
    </path>
    <path>
      <move select="Lead Left" scaleX=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Lead Left" beats="4" scaleX="2.5" scaleY="3.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="6" scaleX="2.5" scaleY="3.5"/>
      <move select="Belle Wheel" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Left" scaleY=".5"/>
    </path>
    <path>
