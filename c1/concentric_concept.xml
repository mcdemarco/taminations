      <move select="Extend Left" beats="4" scaleX="3" scaleY="3"/>
      <move select="Lead Right" scaleY=".5"/>
      <move select="Extend Left" beats="4" scaleX="3.5" scaleY="1.5"/>
      <move select="Lead Left" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2"/>
      <move select="Quarter Left" offsetX="1" offsetY="-.5"/>
      <move select="Extend Right" beats="4" scaleX="3.5" scaleY="2.5"/>
      <move select="Quarter Right" offsetX="1" offsetY=".5"/>
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5"/>
      <move select="Lead Right" beats="1.5" scaleY=".5"/>
      <move select="Extend Right" hands="left" scaleY=".5"/>
      <move select="Lead Left" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5"/>
      <move select="Quarter Left" beats="1.5" offsetX="1" offsetY="-.5"/>
      <move select="Extend Right" hands="left" scaleY=".5"/>
      <move select="Quarter Right" offsetX="1" offsetY=".5"/>
    </path>
  </tam>

  <tam title="From columns, Concentric Extend" group="From columns, Concentric"
