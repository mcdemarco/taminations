    </path>
  </tam>

  <tam title="From columns, Concentric Flutterwheel"
       group="From columns, Concentric"
       formation="Double Pass Thru">
    <path>
      <move select="Stand" hands="nohands" beats="8" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="3.5" />
      <move select="Run Right" hands="right" beats="4" scaleX="2" scaleY="2" offsetX="3" offsetY="-1.5" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="4" />
      <move select="Lead Right" beats="4" scaleX="2.5" scaleY="3" />
      <move select="Lead Right" hands="left" beats="3" scaleX="2.5" scaleY="2.5" />
      <move select="U-Turn Right" hands="left" beats="4" offsetX="3" offsetY="-1.5" />
    </path>
    <path>
      <move select="Stand" hands="nohands" beats="3.5" />
      <move select="Extend Left" hands="right" beats="1.5" scaleY=".5" />
      <move select="Run Right" hands="right" offsetX="1" offsetY="-.5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleY="1.5" />
      <move select="Swing Right" hands="both" scaleX=".5" scaleY=".5" />
      <move select="U-Turn Right" hands="left" offsetX="1" offsetY=".5" />
    </path>
  </tam>

