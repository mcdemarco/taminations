      <move select="Lead Right" beats="2" scaleX="1.5" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Left" scaleX=".5" scaleY=".5"/>
      <move select="U-Turn Left" offsetY="-1"/>
      <move select="Beau Wheel" scaleX=".5" scaleY=".5"/>
      <move select="BackHinge Right" scaleY=".5"/>
    </path>
    <path>
      <move select="Lead Left" scaleX=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Lead Left" beats="2" scaleX=".5" scaleY="1.5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4.5" scaleX=".5" scaleY="1.5"/>
      <move select="Belle Wheel" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Left" scaleY=".5"/>
    </path>
  </tam>

  <tam title="From columns, Concentric Double Star Thru" group="From columns, Concentric"
       formation="Double Pass Thru">
