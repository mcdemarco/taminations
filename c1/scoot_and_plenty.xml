      <move select="Forward" beats="1.5" scaleX="1.5" />
      <move select="Forward 2" beats="5" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Forward" />
      <move select="Extend Right" beats="3" scaleX="3" scaleY=".5" />
      <move select="Lead Right" beats="3" scaleX="2" scaleY="3" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Forward" beats="1.5" scaleX="1.5" />
      <move select="Swing Right" scaleX=".75" scaleY=".75" />
      <move select="Extend Left" beats="1.5" scaleX="1.5" scaleY=".5" />
      <move select="Flip Right" beats="5" offsetX="2" />
      <move select="Extend Left" beats="3" scaleX="3" scaleY=".5" />
      <move select="Forward" />
      <move select="Run Right" scaleY="1.25" />
      <move select="Extend Left" beats="4" scaleX="4" scaleY=".5" />
      <move select="Lead Right" scaleX="1.5" scaleY="1.5" />
      <move select="Lead Right" scaleY=".5" />
    </path>
  </tam>

