<tamination>

  <tam title="Cast Back" from="Right-Hand Box" formation="Box RH">
    <path numbers="1 4">
    </path>
    <path numbers="2 3">
      <move select="Run Left" beats="5" offsetX="-4"/>
    </path>
  </tam>

  <tam title="Cast Back"
       from="Double Pass Thru"
       formation="Double Pass Thru"
       parts="" >
    <path>
      <move select="Forward" beats="3" hands="right" />
    </path>
    <path>
      <move select="Forward" beats="3" hands="left" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="2" />
    </path>
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-2" />
    </path>
  </tam>

  <tam title="Cast Back"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" >
    <path>
      <move select="U-Turn Right" offsetX="-1" offsetY="-2" />
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-1" offsetY="2" />
    </path>
    <path>
      <move select="Forward" beats="3" hands="right" />
    </path>
    <path>
      <move select="Forward" beats="3" hands="left" />
    </path>
  </tam>

  <tam title="Cast Back"
       from="Right-Hand Columns"
       formation="Column RH GBGB"
       parts="" >
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="2" />
    </path>
    <path>
    </path>
    <path>
      <move select="U-Turn Left" offsetX="-2" offsetY="2" />
    </path>
    <path>
    </path>
  </tam>
</tamination>
