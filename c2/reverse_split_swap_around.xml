<tamination>
  
  

  <tam title="Reverse Split Swap Around"
       from="Facing Couples"
       formation="Facing Couples"
       parts="" >
    <path>
      <move select="Quarter Left" beats="3" offsetX="3" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetX="1" offsetY="3" />
    </path>
  </tam>

  <tam title="Reverse Split Swap Around"
       from="Normal Lines"
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Quarter Left" beats="3" offsetX="3" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetX="1" offsetY="2" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetX="3" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetX="1" offsetY="2" />
    </path>
  </tam>

  <tam title="Reverse Split Swap Around"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" >
    <path>
      <move select="Quarter Left" beats="3" offsetX="2" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetY="3" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetX="2" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="3" offsetY="3" />
    </path>
  </tam>

  <tam title="As Couples Reverse Split Swap Around"
       from="Normal Lines"
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="BackHinge Right" beats="4" offsetX="4" />
    </path>
    <path>
      <move select="Hinge Left" beats="4" offsetX="4" />
    </path>
    <path>
      <move select="BackHinge Right" beats="4" offsetY="4" />
    </path>
    <path>
      <move select="Hinge Left" beats="4" offsetY="4" />
    </path>
  </tam>
</tamination>
