<tamination>

  <tam title="Parallelogram Acey Deucey" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path numbers="1 8">
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path numbers="2 7">
      <move select="Swing Left" beats="6"/>
    </path>
    <path numbers="3 6">
      <move select="Swing Left" beats="6"/>
    </path>
    <path numbers="4 5">
      <move select="Run Right" scaleX="2" scaleY="3" beats="6"/>
    </path>
  </tam>

  <tam title="Parallelogram Bend the Line" from="" group="Parallelogram"
       formation="Formation girl -2 1 180 boy -2 -1 180 boy -2 -3 180 girl -2 -5 180">
    <path numbers="1 8">
      <move select="Hinge Left"/>
    </path>
    <path numbers="2 7">
       <move select="BackHinge Right"/>
    </path>
    <path numbers="3 6">
      <move select="BackHinge Left"/>
    </path>
    <path numbers="4 5">
       <move select="Hinge Right"/>
    </path>
  </tam>

  <tam title="Parallelogram Cast Off 3/4" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Cast Right"/>
    </path>
    <path>
      <move select="Cast Right"/>
    </path>
  </tam>

  <tam title="Parallelogram In Roll Circulate" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path>
      <move select="Flip Right"/>
    </path>
    <path>
      <move select="Flip Left"/>
    </path>
    <path>
      <move select="Flip Right"/>
    </path>
  </tam>

  <tam title="Parallelogram Pass the Ocean" from="" group="Parallelogram"
       formation="Formation boy -2 1 0 girl -2 -1 0 boy -2 -3 0 girl -2 -5 0">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Quarter Right" beats="3" offsetX="3" offsetY=".5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Lead Left" beats="3" scaleX="1" scaleY="2.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Quarter Right" beats="3" offsetX="3" offsetY=".5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Lead Left" beats="3" scaleX="1" scaleY="2.5"/>
    </path>
  </tam>

  <tam title="Parallelogram Recycle" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Run Right" beats="4" offsetX="1" offsetY="-1" />
    </path>
    <path>
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".75" offsetX="-.5" />
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".25" offsetX=".5" />
    </path>
    <path>
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".25" offsetX="-.5" />
      <move select="Run Right" beats="2" scaleX=".5" scaleY=".75" offsetX=".5" />
    </path>
    <path>
      <move select="Run Right" beats="4" offsetX="1" offsetY="-3" />
    </path>
  </tam>

  <tam title="Parallelogram Scoot Back" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Extend Left" scaleX="2" scaleY="1.5" beats="2"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5" beats="2"/>
      <move select="Extend Left" scaleX="2" scaleY="2.5" beats="3"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="2" scaleY="1.5" beats="2"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5" beats="2"/>
      <move select="Extend Left" scaleX="2" scaleY="2.5" beats="3"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
  </tam>

  <tam title="Parallelogram Slide Thru" from="" group="Parallelogram"
       formation="Formation boy -2 1 0 girl -2 -1 0 boy -2 -3 0 girl -2 -5 0">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Quarter Right" beats="2" offsetX="1" offsetY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Lead Left" beats="2" scaleX="1" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Quarter Right" beats="2" offsetX="1" offsetY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY="2.5"/>
      <move select="Lead Left" beats="2" scaleX="1" scaleY="1.5"/>
    </path>
  </tam>

  <tam title="Parallelogram Split Circulate" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path>
      <move select="Run Right"/>
    </path>
  </tam>

  <tam title="Parallelogram Walk and Dodge" from="" group="Parallelogram"
       formation="Formation boy -2 1 0  girl -2 -1 180  girl -2 -3 0  boy -2 -5 180">
    <path>
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path>
      <move select="Dodge Right"/>
    </path>
    <path>
      <move select="Extend Left" scaleX="4" scaleY="4" beats="6"/>
    </path>
    <path>
      <move select="Dodge Right"/>
    </path>
  </tam>

</tamination>
