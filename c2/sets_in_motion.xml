<tamination>

  <tam title="Sets in Motion"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Forward 4" />
      <move select="Run Right" beats="4" scaleX="2" scaleY="2" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Quarter Left" />
      <move select="Stand" beats="2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Swing Right" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Sets in Motion"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Quarter Left" offsetX="-.5" offsetY="1"/>
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path>
      <move select="Hinge Right" />
      <move select="Quarter Right" />
      <move select="Stand" beats="2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Forward 4" />
      <move select="Run Left" beats="4" />
      <move select="Forward 2" />
    </path>
  </tam>


  <tam title="Sets in Motion"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH">
    <path numbers="1 8">
      <move select="Forward 4" />
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
      <move select="Forward 2" />
    </path>
    <path numbers="2 7">
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Quarter Left" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path numbers="3 6">
      <move select="Hinge Right" />
      <move select="Quarter Right" />
      <move select="Stand" beats="3" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 3" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path numbers="4 5">
      <move select="Run Right" beats="4" scaleX="2" scaleY="3" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Sets in Motion"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3" />
      <move select="Forward 2" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Quarter Left" />
      <move select="Stand" beats="2" />
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Hinge Left" />
      <move select="Swing Right" />
      <move select="Lead Right" scaleX=".5" />
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path>
      <move select="Forward 4" />
      <move select="Run Left" beats="4" />
      <move select="Forward 2" />
    </path>
  </tam>

  <tam title="Sets in Motion" from="Lines Facing In" formation="Normal Lines">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="6" scaleX="4" scaleY="3"/>
      <move select="Lead Right" scaleX="1.5"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Lead Right"/>
      <move select="Swing Left"/>
      <move select="Quarter Left" offsetX="-.5" offsetY="1"/>
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path>
      <move select="Quarter Left" offsetX="-1" offsetY="1"/>
      <move select="Lead Right" beats="4.5" scaleX=".5"/>
      <move select="Forward 3" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="4" scaleX="4" scaleY="1.5"/>
      <move select="Lead Left" scaleX=".5"/>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Forward 2"/>
    </path>
  </tam>

  <tam title="Sets in Motion" from="Lines Facing Out" formation="Lines Facing Out">
    <path numbers="1 8">
      <move select="Run Left" beats="5" scaleX="2" scaleY="3" />
      <move select="Extend Left" beats="2" scaleX="2"/>
    </path>
    <path numbers="2 7">
      <move select="Quarter Left" offsetX="-1" offsetY="1"/>
      <move select="Swing Left" />
      <move select="Quarter Left" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 3" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path numbers="3 6">
      <move select="Hinge Right" />
      <move select="Quarter Right" />
      <move select="Stand" beats="3" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 3" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path numbers="4 5">
      <move select="Run Right" beats="5" scaleX="3" scaleY="3" />
      <move select="Extend Left" beats="2" scaleX="2"/>
    </path>
  </tam>

  <tam title="Sets in Motion" from="3 and 1 Lines"
       formation="Formation girl -2 3 180 boy -2 1 0 girl -2 -1 180 boy -2 -3 180">
    <path numbers="1 8">
      <move select="Run Left" beats="5" scaleX="2" scaleY="3" />
      <move select="Extend Left" beats="2" scaleX="2"/>
    </path>
    <path numbers="2 7">
      <move select="Hinge Right" />
      <move select="Swing Left" />
      <move select="Quarter Left" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Flip Left" scaleY=".25" />
    </path>
    <path numbers="3 6">
      <move select="Hinge Right" />
      <move select="Quarter Right" />
      <move select="Stand" beats="3" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward 2" />
      <move select="Flip Right" beats="3" offsetX="2" offsetY=".5" />
    </path>
    <path numbers="4 5">
      <move select="Run Right" beats="5" scaleX="3" scaleY="3" />
      <move select="Extend Left" beats="2" scaleX="2"/>
    </path>
  </tam>

  <tam title="Sets in Motion" from="T-Bones"
       formation="Formation boy -3 1 270 girl -3 -1 90 boy -1 -1 180 girl -1 1 180">
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Lead Left" beats="4" scaleX="3" scaleY="2.5"/>
    </path>
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Lead Right" beats="4" scaleX="5" scaleY="3.5"/>
    </path>
    <path>
      <move select="Lead Right" scaleX=".5"/>
      <move select="Quarter Right" />
      <move select="Stand" beats="3" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Right" beats="3" offsetX="1.5" offsetY=".5" />
    </path>
    <path>
      <move select="Quarter Left" offsetX="-.5" offsetY="1"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Quarter Left" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Forward" />
      <move select="Flip Left" offsetX=".5" scaleY=".25" />
    </path>
  </tam>

</tamination>
