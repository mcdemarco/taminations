<tamination>



  <tam title="File to a Line"
       from="Right-Hand Columns"
       formation="Column RH GBGB"
       parts="" >
    <path numbers="2 7">
      <move select="Dodge Left" offsetX="-1" />
    </path>
    <path numbers="4 5">
      <move select="Run Right" offsetX="1" />
    </path>
    <path numbers="6 3">
      <move select="Forward 3" />
    </path>
    <path numbers="8 1">
      <move select="Dodge Left" offsetX="1" />
    </path>
  </tam>

  <tam title="File to a Line"
       from="Left-Hand Columns"
       formation="Column LH GBGB"
       parts="" >
    <path>
      <move select="Dodge Right" offsetX="1" />
    </path>
    <path>
      <move select="Forward 3" />
    </path>
    <path>
      <move select="Run Left" offsetX="1" />
    </path>
    <path>
      <move select="Dodge Right" offsetX="-1" />
    </path>
  </tam>

  <tam title="File to a Line"
       from="Double Pass Thru"
       formation="Double Pass Thru"
       parts="" >
    <path>
      <move select="Dodge Left" offsetX="1" />
    </path>
    <path>
      <move select="Dodge Right" offsetX="1" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

  <tam title="File to a Line"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="" >
    <path>
      <move select="Dodge Right" offsetX="-1" />
    </path>
    <path>
      <move select="Dodge Left" offsetX="-1" />
    </path>
    <path>
      <move select="Flip Left" offsetX="1" />
    </path>
    <path>
      <move select="Run Right" offsetX="1" />
    </path>
  </tam>

  <tam title="File to a Line"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" >
    <path>
      <move select="Dodge Left" offsetX="1" />
    </path>
    <path>
      <move select="Dodge Right" offsetX="1" />
    </path>
    <path>
      <move select="Run Right" offsetX="1" />
    </path>
    <path>
      <move select="Flip Left" offsetX="1" />
    </path>
  </tam>

  <tam title="File to a Line"
       from="Trade By"
       formation="Trade By"
       parts="" >
    <path>
      <move select="Dodge Right" offsetX="-1" />
    </path>
    <path>
      <move select="Dodge Left" offsetX="-1" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Extend Left" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>
</tamination>
