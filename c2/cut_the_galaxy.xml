<tamination>
  
  

  <tam title="Cut the Galaxy"
       from="Right-Hand Galaxy"
       formation="Galaxy RH GP"
       parts="" >
    <path>
      <move select="Lead Right" beats="6" scaleX="2" />
    </path>
    <path>
      <move select="Dodge Right" />
      <move select="Swing Right" />
    </path>
    <path>
      <move select="Extend Left" beats="6" scaleY="2.1" />
    </path>
    <path>
      <move select="Dodge Right" />
      <move select="Swing Right" />
    </path>
  </tam>

  <tam title="Cut the Galaxy"
       from="Left-Hand Galaxy"
       formation="Galaxy LH GP"
       parts="" >
    <path>
      <move select="Extend Right" beats="6" scaleY="2.1" />
    </path>
    <path>
      <move select="Dodge Left" />
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Lead Left" beats="6" scaleX="2" />
    </path>
    <path>
      <move select="Dodge Left" />
      <move select="Swing Left" />
    </path>
  </tam>
</tamination>
