<tamination>

  <tam title="Chisel Thru"
       from="Normal Lines" parts="5;2.5"
       formation="Formation boy 3 2 270  girl 1 2 270  boy -1 2 270  girl -3 2 270">
    <path numbers="7 2">
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2.5" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Extend Left" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path numbers="5 4">
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Right" beats="3" scaleY=".5" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Quarter Left" offsetX="1" offsetY="-.5" />
      <move select="Forward" />
      <move select="Quarter Left" offsetX="1" offsetY="-1" />
    </path>
    <path numbers="3 6">
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Quarter Left" beats="3" offsetX="1" offsetY="-.5" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Lead Right" scaleY=".5" />
      <move select="Extend Left" />
      <move select="Quarter Right" offsetX="1" />
    </path>
    <path numbers="1 8">
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Left" beats="3" scaleX="3" scaleY="1.5" />
      <move select="Extend Left" scaleY=".5" />
      <move select="Quarter Left" offsetX="1" offsetY="-.5" />
      <move select="Forward" />
      <move select="Quarter Left" offsetX="1" offsetY="-1" />
    </path>
  </tam>
</tamination>
