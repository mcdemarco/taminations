<tamination>
  
  

  <tam title="Link Up" from="Two-Faced Lines" formation="Two-Faced Lines RH" parts="2">
    <path>
      <move select="Forward 2" hands="right"/>
      <move select="Run Right" beats="5" offsetX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 2" hands="left"/>
      <move select="Swing Right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Lead Left" beats="2" hands="left" scaleX="2" scaleY="2"/>
      <move select="Quarter Left"/>
      <move select="Forward 2" beats="3.5"/>
    </path>
    <path>
      <move select="Quarter Left" beats="2" hands="left"/>
      <move select="Quarter Left"/>
      <move select="Forward 4" beats="3.5"/>
    </path>
  </tam>

</tamination>
