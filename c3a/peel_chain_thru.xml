<tamination>
  
  

  <tam title="Peel Chain Thru" from="Trade By" formation="Trade By" parts="4;4">
    <path>
      <move select="Run Right" beats="4" offsetX="-2"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Left" scaleY=".5"/>
    </path>
    <path>
      <move select="Run Left" beats="4" offsetX="-2"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Stand" beats="4"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Hinge Left" scaleY=".5"/>
    </path>
    <path>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Right" scaleX=".5" scaleY=".5"/>
      <move select="Swing Left" beats="4" scaleX=".5" scaleY=".5"/>
      <move select="Lead Right" beats="4.5" scaleX="3" scaleY="1.5"/>
    </path>
  </tam>

</tamination>
