<tamination>
  
  

  <tam title="Exchange the Inside Triangles" from="Point-to-Point Diamonds"
       formation="Diamonds RH PTP Girl Points" parts="5;5">
    <path>
      <move select="Lead Right" beats="5" scaleX="2"/>
      <move select="Lead Left" beats="5" scaleY="4"/>
      <move select="Flip Left" beats="5"/>
    </path>
    <path>
      <move select="Lead Left" beats="5" scaleX="2" scaleY="4"/>
      <move select="Flip Left" beats="5" scaleY="1.5"/>
      <move select="Lead Left" beats="5" scaleX="2"/>
    </path>
    <path>
      <move select="Flip Right" beats="5"/>
      <move select="Lead Right" beats="5" scaleX="2"/>
      <move select="Lead Left" beats="5" scaleY="4"/>
    </path>
    <path>
    </path>
  </tam>

