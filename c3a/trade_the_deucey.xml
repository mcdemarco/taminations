<tamination>
  
  

  <tam title="Trade the Deucey"
       from="Normal Lines"
       formation="Normal Lines">
    <path>
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" scaleX="3" scaleY="2" beats="4.5" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Flip Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Extend Left" scaleX="3" scaleY="2" beats="4.5" />
      <move select="Forward" beats="1.5" />
    </path>
  </tam>

  <tam title="Trade the Deucey"
       from="Lines Facing Out"
       formation="Lines Facing Out">
    <path>
      <move select="Run Left" scaleX="2" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Flip Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleX="3" scaleY="2" beats="6" />
    </path>
  </tam>

  <tam title="Trade the Deucey"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH">
    <path>
      <move select="Extend Right" scaleX="4" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
  </tam>

  <tam title="Trade the Deucey"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH">
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Extend Left" scaleX="4" scaleY="2" beats="6" />
    </path>
  </tam>

  <tam title="Trade the Deucey"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGGB">
    <path>
      <move select="Extend Right" scaleX="4" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleX="2" scaleY="2" beats="6" />
    </path>
  </tam>

  <tam title="Trade the Deucey"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGGB">
    <path>
      <move select="Run Left" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Run Right" scaleX="2" scaleY="2" beats="6" />
    </path>
    <path>
      <move select="Extend Left" scaleX="4" scaleY="2" beats="6" />
    </path>
  </tam>

