<tamination>
  
  

  <tam title="Keep Busy" from="Right-Hand Two-Faced Lines" formation="Two-Faced Lines RH" parts="2;1.5;3">
    <path>
      <move select="Forward 2" hands="right"/>
      <move select="Stand" beats="1.5"/>
      <move select="Run Right" scaleY="1.2"/>
      <move select="Extend Right" beats="2.5" scaleX="2" scaleY=".4"/>
    </path>
    <path>
      <move select="Forward 2" hands="left"/>
      <move select="Hinge Right"/>
      <move select="Lead Right" beats="3" scaleX="1.8"/>
      <move select="Flip Right" beats="2.5" offsetX="2" scaleY="0.4"/>
    </path>
    <path>
      <move select="Run Right" hands="left" beats="5"/>
      <move select="Dodge Left" beats="4"/>
    </path>
    <path>
      <move select="Run Right" beats="5" hands="right" scaleX="3" scaleY="3"/>
      <move select="Forward 4"/>
    </path>
  </tam>

</tamination>
