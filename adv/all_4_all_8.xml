    </path>
    <path numbers="2 7">
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Lead Right" hands="right" beats="2" scaleX="1.5" />
      <move select="Swing Left" scaleY=".75" />
    </path>
    <path numbers="5 4">
      <move select="Quarter Right" beats="5" offsetY="-1" />
      <move select="Swing Left" />
    </path>
    <path numbers="3 6">
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Lead Right" hands="right" beats="2" scaleX="1.5" />
      <move select="Swing Left" scaleY=".75" />
    </path>
  </tam>

