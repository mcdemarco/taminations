      <move select="Forward" hands="right" />
      <move select="Lead Right" hands="left" beats="4" scaleX="2" scaleY="3" />
      <move select="Forward 2" />
      <move select="Lead Right" hands="right" beats="2" scaleY="2" />
      <move select="Lead Right" hands="left" beats="4" scaleX="2" scaleY="3" />
