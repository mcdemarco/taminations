<tamination>

  <comment>
    <p>All 4 Couples / All 8 versions of Advanced and Challenge calls are listed with each call.</p>
  </comment>

  <tam title="All 4 Couples Chase Right" group="All 4 Couples"
       from=""
       formation="Static Facing Out"
       parts="" >
    <path numbers="2 7">
      <move select="Run Right" beats="4" scaleY="2" offsetX="-1" />
      <move select="Forward 3" />
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path numbers="1 8">
      <move select="Run Right" offsetX="-2" />
      <move select="Forward 3" />
      <move select="Run Right" beats="4" scaleY="2" offsetX="1" />
    </path>
    <path numbers="3 6">
      <move select="Run Right" beats="4" scaleY="2" offsetX="-1" />
      <move select="Forward 3" />
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path numbers="5 4">
      <move select="Run Right" offsetX="-2" />
      <move select="Forward 3" />
      <move select="Run Right" beats="4" scaleY="2" offsetX="1" />
    </path>
  </tam>

  <tam title="All 4 Couples Extend" group="All 4 Couples"
       from=""
