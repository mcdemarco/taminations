<tamination>
  
  
  

  <tam title="Pair Off"
       from="Facing Couples"
       formation="Facing Couples"
       parts="" >
    <path>
      <move select="Lead Left" beats="2" />
    </path>
    <path>
      <move select="Lead Right" beats="2" />
    </path>
  </tam>

  <tam title="Pair Off"
       from="Lines"
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="Quarter Left" beats="2" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="1" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="1" />
    </path>
  </tam>

  <tam title="Pair Off"
       from="Eight Chain Thru"
       formation="Eight Chain Thru"
       parts="" >
    <path>
      <move select="Quarter Left" beats="2" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetY="-1" />
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetY="1" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetY="-1" />
    </path>
  </tam>

  <tam title="As Couples Pair Off"
       from="Lines"
       formation="Normal Lines"
       parts="" >
    <path>
      <move select="BackHinge Right" />
    </path>
    <path>
      <move select="Hinge Left" />
    </path>
    <path>
      <move select="Hinge Right" />
    </path>
    <path>
      <move select="BackHinge Left" />
    </path>
  </tam>

  <tam title="All 4 Couples Pair Off"
       from="Static Square"
       formation="Static Square"
       parts="" >
    <path>
      <move select="Eighth Left" beats=".5" />
      <move select="Forward" beats="3" scaleX="2.828" scaleY="2.828" />
      <move select="Eighth Left" beats=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Eighth Left" beats=".5" />
      <move select="Forward" beats="3" scaleX="2.828" scaleY="2.828" />
      <move select="Eighth Left" beats=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="2" scaleY="2" />
    </path>
  </tam>
</tamination>
