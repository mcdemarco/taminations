<tamination>

  <tam title="Cross Over Circulate"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="3" >
    <path numbers="1 8">
      <move select="Extend Right" beats="3" scaleX="1.5" />
      <move select="Extend Right" beats="3" scaleX="2.5" />
    </path>
    <path numbers="2 7">
      <move select="Extend Left" beats="3" scaleX="2.5" />
      <move select="Extend Left" beats="3" scaleX="1.5" />
    </path>
    <path numbers="3 6">
      <move select="Quarter Right" offsetX=".1" offsetY="-1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Right" offsetX="1" offsetY="-.1" />
    </path>
    <path numbers="4 5">
      <move select="Quarter Right" offsetX=".1" offsetY="-1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Right" offsetX="1" offsetY="-.1" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH"
       parts="3" >
    <path numbers="1 8">
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path numbers="2 7">
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path numbers="3 6">
      <move select="Extend Right" beats="3" scaleX="1.5" />
      <move select="Extend Right" beats="3" scaleX="2.5" />
    </path>
    <path numbers="4 5">
      <move select="Extend Left" beats="3" scaleX="2.5" />
      <move select="Extend Left" beats="3" scaleX="1.5" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="3" >
    <path>
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path>
      <move select="Quarter Left" offsetX=".1" offsetY="1" />
      <move select="Forward 2" beats="3" />
      <move select="Quarter Left" offsetX="1" offsetY=".1" />
    </path>
    <path>
      <move select="Lead Right" />
      <move select="Forward 2" beats="3" />
      <move select="Lead Right" />
    </path>
    <path>
      <move select="Lead Right" />
      <move select="Forward 2" beats="3" />
      <move select="Lead Right" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Inverted Lines"
       formation="Inverted Lines Centers In"
       parts="" >
    <path>
      <move select="Flip Left" beats="6" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4.5" scaleX="3" scaleY="2" />
      <move select="Forward" beats="1.5" />
    </path>
    <path>
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleY="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Right-Hand Waves"
       formation="Ocean Waves RH BGBG"
       parts="3" >
    <path numbers="1 8">
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path numbers="2 7">
      <move select="Run Left" beats="6" scaleX=".5" scaleY="2" />
    </path>
    <path numbers="3 6">
      <move select="Forward" beats="1.5" />
      <move select="Extend Right" beats="4.5" scaleX="3" scaleY="2" />
    </path>
    <path numbers="4 5">
      <move select="Run Right" beats="6" scaleX="1.5" scaleY="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate"
       from="Left-Hand Waves"
       formation="Ocean Waves LH BGBG"
       parts="3" >
    <path>
      <move select="Run Left" beats="6" scaleX=".5" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2.5" scaleY="2" />
      <move select="Forward" scaleX="1.5" beats="2" />
    </path>
    <path>
      <move select="Run Right" beats="6" scaleX="1.5" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="2.5" scaleY="2" />
      <move select="Forward" scaleX="1.5" beats="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="T-Bones 1"
       formation="T-Bone DLRD" >
    <path>
      <move select="Run Left" beats="5" scaleY="2" />
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Dodge Left"/>
    </path>
    <path>
      <move select="Dodge Right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Run Right" beats="5" scaleX="2" scaleY="2" />
    </path>
  </tam>

  <tam title="Cross Over Circulate" from="T-Bones 2"
       formation="T-Bone ULRU" >
    <path>
      <move select="Dodge Right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Dodge Left"/>
    </path>
    <path>
      <move select="Dodge Right"/>
      <move select="Forward 2"/>
    </path>
    <path>
      <move select="Forward 2"/>
      <move select="Dodge Left"/>
    </path>
  </tam>

</tamination>
