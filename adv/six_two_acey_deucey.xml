<tamination>
  
  
  

  <tam title="Six-Two Acey Deucey"
       from="Diamonds"
       formation="Diamonds RH Girl Points"
       parts="" >
    <path>
      <move select="Lead Right" beats="4" scaleX="3" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Swing Left" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleY="3" />
    </path>
  </tam>

  <tam title="Six-Two Acey Deucey"
       from="Point-to-Point Diamonds"
       formation="Diamonds RH PTP Girl Points"
       parts="" >
    <path>
      <move select="Extend Left" beats="2" scaleX="3" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="3" scaleY=".5" />
    </path>
    <path>
      <move select="Swing Left" scaleX=".5" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleY="2" />
    </path>
  </tam>

  <tam title="Six-Two Acey Deucey"
       from="Hourglass"
       formation="Hourglass RH BP"
       parts="" >
    <path>
      <move select="Lead Right" beats="4" scaleY="3" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX="3" />
    </path>
    <path>
      <move select="Forward 4" />
    </path>
    <path>
      <move select="Swing Right" />
    </path>
  </tam>

  <tam title="Six-Two Acey Deucey"
       from="Wave and Mini-Waves"
       formation="Formation boy -3 -1 180 girl 0 -3 180 boy 3 -1 180 girl 0 -1 0"
       parts="" >
    <path>
      <move select="Flip Right" />
    </path>
    <path>
      <move select="Extend Right" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="4" scaleX="3" scaleY="2" />
    </path>
    <path>
      <move select="Swing Left" />
    </path>
  </tam>
</tamination>
